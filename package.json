  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "node-hue-api": "^4.0.10",
    "pug": "^2.0.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "request": "^2.88.0",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
